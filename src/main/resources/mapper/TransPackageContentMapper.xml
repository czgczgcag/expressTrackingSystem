<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.expresstracking.dao.TransPackageContentDao">
<insert id="insert" parameterType="TransPackageContent">
    insert into transpackagecontent values (#{expressid} ,#{packageid},#{status} );
</insert>

    <update id="update" parameterType="TransPackageContent">
        update transpackagecontent set ExpressID=#{expressid},PackageID=#{packageid},Status=#{status};
    </update>
</mapper>
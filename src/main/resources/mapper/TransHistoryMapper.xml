<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.expressTracking.dao.TransHistoryDao">

    <select id="get" parameterType="String" resultType="TransHistory">
        select * from transhistory where PackageID=#{id};
    </select>

    <insert id="insert" parameterType="TransHistory">
        insert into transhistory(packageid, acttime, uidfrom, uidto, x, y) values(#{PackageId}, #{actTime} ,#{UIDFrom}, #{UIDto}, #{x} ,#{y} );
    </insert>

</mapper>